mixin list_of_works(class_adit, id_block)
    // Блок 5 - список примеров работ кампании
    div(class="list_of_works "+class_adit id=id_block)
        h2 Наши работы
        div.works_sheat
            -let n = 0
            while n < 8
                div(class="work_image w_img"+n)
                - n++

